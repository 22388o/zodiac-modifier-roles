{
  "name": "zodiac-modifier-roles",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ zodiac-modifier-roles",
    "create-local": "graph create --node http://localhost:8020/ zodiac-modifier-roles",
    "remove-local": "graph remove --node http://localhost:8020/ zodiac-modifier-roles",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 zodiac-modifier-roles",
    "getabi:permissions": "cp ../evm/build/artifacts/contracts/Permissions.sol/Permissions.json ./abis/Permissions.json",
    "getabi:roles": "cp ../evm/build/artifacts/contracts/Roles.sol/Roles.json ./abis/Roles.json",
    "prebuild": "yarn getabi:permissions && yarn getabi:roles && yarn codegen",
    "fmt": "prettier '(test|src)/**/*.ts' -w",
    "pre-commit": "yarn fmt"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.25.2",
    "@graphprotocol/graph-ts": "0.24.1"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}
